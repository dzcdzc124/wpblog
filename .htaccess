
# BEGIN WordPress
<IfModule mod_rewrite.c>
	RewriteEngine On

	RewriteCond %{HTTP_HOST} ^toucanz.com/ [NC]
	RewriteRule ^(.*)$ http://www.toucanz.com$1 [L,R=301]

	RewriteCond %{HTTP_HOST} ^blog.toucanz.com/ [NC]
	RewriteRule ^(.*)$ http://www.toucanz.com$1 [L,R=301]

	RewriteBase /
	RewriteRule ^index\.php$ - [L]

	RewriteCond %{REQUEST_FILENAME} !-f
	RewriteCond %{REQUEST_FILENAME} !-d
	RewriteRule . /index.php [L]
</IfModule>

# END WordPress
